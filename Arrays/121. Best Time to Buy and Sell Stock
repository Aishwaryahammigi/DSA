class Solution {
    public int maxProfit(int[] prices) {
         int maxProfit = 0;
        int mini = prices[0];
        for (int i = 1; i < prices.length; i++) {
            int curProfit = prices[i] - mini;
            if (curProfit > maxProfit) {
                maxProfit = curProfit;
            }
            if (prices[i] < mini) {
                mini = prices[i];
            }
        }
        return maxProfit;
    }
}
